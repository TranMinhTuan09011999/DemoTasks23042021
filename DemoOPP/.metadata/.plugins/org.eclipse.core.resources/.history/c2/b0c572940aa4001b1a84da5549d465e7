package com.itsj.encapsulation;

public class Student {
	private int studentId;
    private String name;
    
    public int getId() {
        return StudentId;
    }
    public void setId(int id) {
        this.StudentId = id;
    }
    public String getname() {
        return name;
    }
    public void setname(String name) {
        this.name = name;
    }
}
